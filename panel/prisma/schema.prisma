generator client {
    provider = "prisma-client"
    output   = "generated/prisma"
}

datasource db {
    provider = "postgresql"
    // url      = env("DATABASE_URL")
}

model Post {
    id        Int      @id @default(autoincrement())
    name      String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@index([name])
}
